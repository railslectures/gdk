- @no_container = true
- page_title "Users"

%div{ class: container_class }
  .top-area
    .prepend-top-default
      %h3 Source Code Projects
  %ul.users-list.content-list
    - if Project.count == 0
      %li
        .nothing-here-block No projects found.
    - else
      = render partial: 'help/user', collection: @users